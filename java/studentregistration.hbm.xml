<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
<class name="studentjsp.Studentregistration" table="registrationstudent">
<id name="id">
<generator class="assigned"></generator>
</id>
<property name="studentname"></property>
<property name="course"></property>
<property name="gender"></property>
<property name="dateofbirth"></property>
<property name="mobile"></property>
<property name="address"></property>
<property name="email"></property>
<property name="password"></property>
<property name="confirmpassword"></property>
</class>
</hibernate-mapping>